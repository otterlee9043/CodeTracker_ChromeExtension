{
  "name": "Timestamp",
  "description": "Show current timestamp of the youtube video",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' https://www.youtube.com; object-src 'self'",
  "permissions": ["storage", "activeTab", "tabs", "https://www.youtube.com/"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Default_Timestamp",
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  },
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  },
  "options_page": "options.html"
}
